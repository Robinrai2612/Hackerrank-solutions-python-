from collections import Counter

def makingAnagrams(s1, s2):
    # Write your code here
    dict1=Counter(s1)
    dict2=Counter(s2)
    #if len(dict2)>len(dict1):
    equal1=dict2-dict1
        #return len((equal.keys())*2
    #else:
    equal2=dict1-dict2
    return sum(equal1.values())+sum(equal2.values())
